#!/bin/bash

set -e

pushd "$( dirname "${BASH_SOURCE[0]}" )/.." >/dev/null

mkdir -p csv_schemas

for f in ./highway_data_service_counts_scrape/short_count_volume*; do
  zcat "$f" |
    head -1 |
    sed 's/"//g; s/[[:space:]]*//g; /^$/d;'
done |
  sort -u |
  tr ',' '\n' |
  nl -ba > ./csv_schemas/short_count_volume

popd >/dev/null
